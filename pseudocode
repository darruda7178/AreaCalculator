def SquareArea
  ask for sidelength
  calculate area = sidelength*sidelength
  squareArea
def RectangleArea
  ask for length
  ask for width
  calculate area = length*width
  RectangleArea
def CircleArea
  ask for radius
  calculate area = pi*radius**2
def TriangleArea
  ask for base
  ask for height
  calculate area = base*height / 2
  TriangleArea
